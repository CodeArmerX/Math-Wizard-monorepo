{
  "fmt": {
    "singleQuote": true,
    "semiColons": false
  },
  "unstable": [
    "kv"
  ],
  "imports": {
    "hono": "jsr:@hono/hono@^4.4.2"
  },
  "tasks": {
    "start": "deno serve --watch ./src/main.ts",
    "dev": "deno run --watch --unstable --allow-net --allow-read ./src/main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}