{
  "fmt": {
    "singleQuote": true,
    "semiColons": false
  },
  "unstable": [
    "kv"
  ],
  "imports": {
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "hono": "jsr:@hono/hono@^4.4.2",
    "ulid": "https://deno.land/x/ulid@v0.3.0/mod.ts",
    "zod": "https://deno.land/x/zod@v3.23.8/mod.ts"
  },
  "tasks": {
    "start": "deno serve --watch --allow-env --allow-net --allow-read ./src/main.ts",
    "dev": "deno run --watch --unstable --allow-net --allow-read ./src/main.ts"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "hono/jsx"
  }
}
